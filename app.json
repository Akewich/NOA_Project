{
  "expo": {
    "name": "noa-project",
    "slug": "noa-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/NOA.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/NOA.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFBFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/NOA.png",
        "backgroundColor": "#FAFBFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/NOA.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": {
      "Inter_18pt-Medium": "./assets/fonts/Inter_18pt-Medium.ttf",
      "Koulen-Regular": "./assets/fonts/Koulen-Regular.ttf"
    }
  }
}
